                                    "town": {
                                        "type": "string"
                                    },
                                    "state": {
                                        "type": "string"
                                    },
                                    "uf": {
                                        "type": "string"
                                    },
                                    "phone1": {
                                        "type": "string"
                                    },
                                    "phone2": {
                                        "type": "string"
                                    },
                                    "cel": {
                                        "type": "string"
                                    },
                                    "contact": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "personal_id": {
                                        "type": "string"
                                    },
                                    "social_id": {
                                        "type": "string"
                                    },
                                    "car": {
                                        "type": "string"
                                    },
                                    "license": {
                                        "type": "string"
                                    }
                                }
                            },
